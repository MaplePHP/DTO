    "dom",
    "string",
    "number",
    "date"
  ],
  "homepage": "https://maplephp.github.io/DTO/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Daniel Ronkainen",
      "email": "daniel.ronkainen@wazabii.se"
    },
    {
      "name": "MaplePHP",
      "homepage": "https://wazabii.se"
    }
  ],
  "require": {
    "php": ">=8.0",
    "maplephp/swiftrender": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "MaplePHP\\DTO\\": "src"
    }
